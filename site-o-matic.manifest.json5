{
  rootDomainName: "konker.dev",
  webmasterEmail: "webmaster@konker.dev",
  registrar: "dynadot",
  protected: false,
  webHosting: {
    type: "cloudfront-s3",
  },
  pipeline: {
    type: "codestar-custom",
    codestarConnectionArn: "arn:aws:codestar-connections:us-east-1:919976498930:connection/8ec16bf6-9d71-49f1-81ff-6d7be5df7702",
    owner: "morningwoodsoftware",
    repo: "konker.dev",
    buildImage: "aws/codebuild/amazonlinux2-x86_64-standard:5.0",
    buildFiles: ["www/**/*"],
    buildPhases: {
      install: {
        "runtime-versions": {
          "nodejs": "20.x"
        },
        "on-failure": "ABORT",
        commands: [
          "npm install -g pnpm",
          "pnpm install"
        ],
      },
      build: {
        "on-failure": "ABORT",
        commands: ["pnpm run build"],
      },
    },
  },
  dns: {
    domainName: "konker.dev",
    extraDnsConfig: [
      {
        type: "MX",
        hostName: "in1-smtp.messagingengine.com",
        priority: 10,
      },
      {
        type: "MX",
        hostName: "in2-smtp.messagingengine.com",
        priority: 20,
      },
      {
        type: "CNAME",
        recordName: "fm1._domainkey.konker.dev",
        domainName: "fm1.konker.dev.dkim.fmhosted.com",
      },
      {
        type: "CNAME",
        recordName: "fm2._domainkey.konker.dev",
        domainName: "fm2.konker.dev.dkim.fmhosted.com",
      },
      {
        type: "CNAME",
        recordName: "fm3._domainkey.konker.dev",
        domainName: "fm3.konker.dev.dkim.fmhosted.com",
      },
      {
        type: "TXT",
        recordName: "",
        values: ["v=spf1 include:spf.messagingengine.com ?all"],
      },
    ],
  },
}
