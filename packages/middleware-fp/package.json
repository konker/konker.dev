{
  "name": "@konker.dev/middleware-fp",
  "version": "0.0.19",
  "author": "Konrad Markus <mail@konker.dev>",
  "description": "Type safe middleware system based on Effect-ts",
  "license": "ISC",
  "files": [
    "dist/**/*",
    "!dist/**/*.test.*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/konkerdotdev/middleware-fp"
  },
  "keywords": [
    "middleware",
    "node",
    "typescript",
    "functional-programming",
    "effect-ts"
  ],
  "peerDependencies": {
    "@aws-sdk/client-dynamodb": "^3.637.0",
    "@gomomento/sdk": "^1.95.0",
    "aws-lambda": "^1.0.7"
  },
  "dependencies": {
    "@effect/schema": "^0.72.4",
    "@konker.dev/aws-client-effect-dynamodb": "workspace:*",
    "@konker.dev/effect-ts-prelude": "workspace:*",
    "@konker.dev/momento-cache-client-effect": "workspace:*",
    "@konker.dev/tiny-auth-utils-fp": "workspace:*",
    "@konker.dev/tiny-cache-fp": "workspace:*",
    "@konker.dev/tiny-error-fp": "workspace:*",
    "@konker.dev/tiny-utils-fp": "workspace:*",
    "effect": "^3.11.3",
    "lesslog": "^0.2.7"
  },
  "devDependencies": {
    "@effect/language-service": "^0.0.21",
    "@types/aws-lambda": "^8.10.146",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.9",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@typescript-eslint/typescript-estree": "^6.21.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-deprecation": "^2.0.0",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-sort-destructure-keys": "^1.6.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "leasot": "^13.3.0",
    "madge": "^6.1.0",
    "prettier": "^3.4.2",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "type-coverage": "^2.29.7",
    "typescript": "^5.7.2"
  },
  "scripts": {
    "test": "jest --no-cache -w 4",
    "clean": "rm -rf dist && rm -f tsconfig.tsbuildinfo",
    "typecheck": "tsc --noEmit",
    "build": "pnpm run clean && tsc",
    "type-coverage-check": "type-coverage --detail --at-least 99 --ignore-files **/*.test.ts --ignore-files src/test/*",
    "circular-check": "madge --extensions ts --circular --no-color --no-spinner --warning --ts-config ./tsconfig.json src",
    "eslint-check": "eslint --max-warnings=0 '**/*.{ts,js,json}'",
    "fixme-check": "leasot --exit-nicely --skip-unsupported --ignore node_modules src",
    "eslint-fix": "npx eslint --fix||true",
    "prettier-check": "npx prettier --check --ignore-path .gitignore --ignore-path .prettierignore '**/*.{css,html,js,ts,json,md,yaml,yml}'",
    "prettier-fix": "pnpm run prettier-check --write",
    "lint-check": "pnpm run eslint-check && pnpm run prettier-check && pnpm run circular-check && pnpm run fixme-check",
    "lint-fix": "pnpm run eslint-fix && pnpm run prettier-fix",
    "pre-push": "pnpm run lint-check && pnpm run typecheck",
    "ci": "pnpm run lint-check && pnpm run typecheck && pnpm run type-coverage-check && pnpm run test && pnpm run build && pnpm run fixme-check",
    "cd": "pnpm run build && pnpm publish --access public"
  }
}
